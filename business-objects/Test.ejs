TABLE <%= schema %>.C_USER {
  id UUID
  human_Id UUID
  email VARCHAR
  first_Name VARCHAR
  last_Name VARCHAR
  profile_Image VARCHAR
  start_Date TIMESTAMP
  end_Date TIMESTAMP
  is_Current BOOLEAN
  created_At TIMESTAMP
  created_By_Id UUID
  last_Modified_At TIMESTAMP
  last_Modified_By_Id UUID

  Indexes {
      (id) [pk, name:"pk"]
  }
}


